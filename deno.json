{
  "name": "@app/shopping-cart",
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": [
      "unplugin-fonts/client"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "deno.ns",
      "deno.unstable",
      "node"
    ]
  },
  "test": {
    "permissions": "default"
  },
  "permissions": {
    "default": {
      "net": true
    }
  },
  "tasks": {
    "dev": "dx vite dev",
    "test": "dx vitest run"
  },
  "exports": {},
  "imports": {
    "@fontsource/roboto": "npm:@fontsource/roboto@^5.2.9",
    "@std/assert": "jsr:@std/assert@1",
    "@deno/vite-plugin": "npm:@deno/vite-plugin@1.0.6",
    "@vitejs/plugin-vue": "npm:@vitejs/plugin-vue@6.0.4",
    "@vue/test-utils": "npm:@vue/test-utils@2.4.6",
    "happy-dom": "npm:happy-dom@20.5.0",
    "unplugin-fonts": "npm:unplugin-fonts@^1.4.0",
    "vite": "npm:vite@7.3.1",
    "vite-plugin-vue-devtools": "npm:vite-plugin-vue-devtools@8.0.5",
    "vite-plugin-vuetify": "npm:vite-plugin-vuetify@2.1.3",
    "vitest": "npm:vitest@4.0.18",
    "vue": "npm:vue@3.5.27",
    "@/": "./src/",
    "@public/": "./public/",
    "vuetify": "npm:vuetify@3.11.8",
    "@mdi/font": "npm:@mdi/font@^7.4.47",
    "pinia": "npm:pinia@3.0.4"
  },
  "exclude": [
    ".vite/",
    "public/",
    "dist/"
  ]
}
